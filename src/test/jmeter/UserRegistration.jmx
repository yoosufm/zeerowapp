            <stringProp name="script"> // if(vars.get(&quot;status&quot;).contains(&quot;true&quot;)){
            try {
 			 String root = new  java.io.File(&quot;.&quot;).getCanonicalPath() + &quot;/&quot;;
 			 vars.put(&quot;baseDir&quot;, root);
                FileWriter fw = new FileWriter(vars.get(&quot;baseDir&quot;) + vars.get(&quot;paymentIdFile&quot;),true); //the true will append the new data
                fw.write(vars.get(&quot;uniqueId&quot;) + &quot;\t&quot; + vars.get(&quot;userId&quot;) );
                fw.write(&quot;\n&quot;);
                fw.close();

                BufferedReader br = new BufferedReader(new FileReader(vars.get(&quot;baseDir&quot;) + vars.get(&quot;paymentIdFile&quot;)));	
                StringBuilder sb = new StringBuilder();               
                String line = br.readLine();             

                while (line != null) {
                	
                    sb.append(line);                  
                    //sb.append(System.lineSeparator());                   
                    line = br.readLine();
                   
                }
                br.close();               
                String everything = sb.toString();            
                everything = everything.replace(&quot;\n&quot;, &quot;&quot;).replace(&quot;\r&quot;, &quot;&quot;);
                everything = everything.substring(0,everything.length()-1);

                vars.put(&quot;paymentIDs&quot;, everything);
               //System.out.print(vars.get(&quot;paymentIDs&quot;).split(&quot;,&quot;).length + &quot;,&quot;);

            }catch (Exception e){
                System.out.println(&quot;0000000000000000 ====================== &quot; +  e.printStackTrace());
            }

  //      }
</stringProp>
